{
    "editor.rulers": [
        {
            "column": 80
        }
    ],
    "editor.renderWhitespace": "boundary",
    "editor.formatOnSave": true,
    "cmake.generator": "Ninja",
    "cmake.debugConfig": {
        "cwd": "${command:cmake.buildDirectory}"
    },
    "files.exclude": {
        "**/*.rpyc": true,
        "**/*.rpa": true,
        "**/*.rpymc": true,
        "**/cache/": true
    }
}